{
  "name": "@seed-fe/request",
  "version": "1.0.0",
  "description": "A powerful and flexible HTTP request library based on Axios",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "check": "biome check --write",
    "format": "biome format --write",
    "build": "tsup",
    "test": "echo \"Tests temporarily skipped\" && exit 0"
  },
  "keywords": [
    "axios",
    "http",
    "http-client",
    "request",
    "typescript",
    "browser",
    "node"
  ],
  "author": "XiangHongAi <xianghongai@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xianghongai/seed-fe-request.git"
  },
  "bugs": {
    "url": "https://github.com/xianghongai/seed-fe-request/issues"
  },
  "homepage": "https://github.com/xianghongai/seed-fe-request#readme",
  "packageManager": "pnpm@10.6.5",
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "axios": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
